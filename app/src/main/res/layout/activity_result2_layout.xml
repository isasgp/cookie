<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="420dp"
        android:layout_height="100dp"
        android:layout_gravity="center|top"
        android:layout_marginTop="-20dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/real_cookie_bar" />


    <ImageView
        android:id="@+id/result_1"
        android:layout_width="431dp"
        android:layout_height="76dp"
        android:layout_gravity="center|top"
        android:layout_marginTop="100dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/result_2" />

    <ImageView
        android:id="@+id/result_image"
        android:layout_width="357dp"
        android:layout_height="320dp"
        android:layout_gravity="center"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/mina_bold"
        app:srcCompat="@drawable/result_image" />

    <ImageView
        android:id="@+id/result_view"
        android:layout_width="378dp"
        android:layout_height="142dp"
        android:layout_gravity="center"
        android:layout_marginTop="250dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/result_view" />

    <ImageButton
        android:id="@+id/homeButton2"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:background="@android:color/transparent"
        android:text="홈으로"
        app:srcCompat="@drawable/real_home_button"
        android:layout_marginTop="5dp"/>

    <TextView
        android:id="@+id/pet_name"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center|top"
        android:layout_marginTop="650dp"
        android:text="TextView"
        android:fontFamily="@font/mina_bold"
        android:textColor="@color/black"
        android:textSize="25dp" />

    <ImageButton
        android:id="@+id/button"
        android:layout_marginTop="710dp"
        android:layout_marginLeft="120dp"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_gravity="top|center"
        app:srcCompat="@drawable/reslult_button"
        android:background="@android:color/transparent"
        android:text="다음 페이지" />
</FrameLayout>